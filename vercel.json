{
  "version": 2,
  "builds": [
    {
      "src": "portfolio-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "portfolio-frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "portfolio-frontend/dist/$1"
    }
  ]
}